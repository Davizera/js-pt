      var texto ='{"A putaria é muito boa, mas precisa ser remaneja, para que ela não afete seus estudo gerencie bem o seu tempo, por isso durma pouco, estude bastante, tire boas notas para que não tenham o que falar de você e seja um exemplo para todos ao seu redor"}';

      console.log(texto);

      var texto2='{"paragrafo":"curtir é legal, mas estudar é melhor e dá futuro"}';

      console.log(texto2); // para o JS isso é uma string, por causa das ''
      var objTexto = JSON.parse(texto2);
      console.log(objTexto); //para o JS isso é um objeto

